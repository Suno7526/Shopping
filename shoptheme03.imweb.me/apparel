			$(function() {
				setTimeout(function(){
					$('.se-pre-con').remove();
				}, 500);
				$('a._fade_link').on('click.fadeClick', function(e){
					var that = $(this);
					var link = that.attr('href');
					var link_type = link.indexOf('#') < 0 && link.indexOf('javascript:;') < 0;
					var link_attr = that.attr('target') != '_blank' && that.attr('onclick') == undefined && !$(e.target).hasClass('_toggle_btn') && that.data('is_folder_menu') !== 'Y';

					if (link_attr && link_type) {
						var body = $('body');
						body.fadeTo(400, 0, function(){
							window.location.href = link;
						});
						body.css('background-color','#fff');
						e.preventDefault();
					}
				})
			});
